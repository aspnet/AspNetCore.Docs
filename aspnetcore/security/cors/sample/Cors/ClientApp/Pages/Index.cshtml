@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Cors Test</h1>
</div>

<div>
    <input type="button" value="Test" onclick="requestVal()" />
    <span id='result'></span>
</div>

<script>
function requestVal() {
    const resultSpan = document.getElementById('result');

    fetch('https://webapi20190128063025.azurewebsites.net/api/values/1')
        .then(response => response.json())
        .then(data => resultSpan.innerText = data)
        .catch(error => resultSpan.innerText = 'See F12 Console for error');
}
</script>